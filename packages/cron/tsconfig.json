{
  "compilerOptions": {
      "target": "ESNext",
      "module": "ESNext",
      "lib": ["ESNext"],
      "types": ["@cloudflare/workers-types"],
      "moduleResolution": "Node",
      "experimentalDecorators": true,
      "strictPropertyInitialization": false,
      "strict": true,
      "baseUrl": "./",
      "paths": {
          "@polyratings/backend/*": ["src/*"]
      },
      "declaration": true,
      "noEmit": true,
      "skipLibCheck": true,
      "noEmitHelpers": true
  },
  "include": ["src/**/*"]
}
